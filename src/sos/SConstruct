Import("*")

srclist = "main.c frames.c libsos.c pager.c timer.c network.c frames_test.c io.c io_serial.c sysent.c process.c datastructures/circular_buffer.c"
liblist = "l4 c ixp_osal ixp400_xscale_sw lwip nfs serial sos_shared"

obj = env.KengeProgram("sos", source = Split(srclist), LIBS = Split(liblist))
Return("obj")

